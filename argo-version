#!/usr/bin/env bash
script="$(basename "$0")"
scriptPath="$(cd -- "$(dirname -- "${BASH_SOURCE[0]}")" &> /dev/null && pwd)"
scriptsPath="$scriptPath/src/scripts"
script="$scriptPath/$script"
echo "$script: Starting."
"$scriptsPath/devspace/setup/set-env-vars.sh"
pushd "$KINDEST_ARGO_CD_PROJECT_ROOT" || exit
"$scriptsPath/forward-port.sh" &
argocd version
popd || exit
echo "$script: Finished."
